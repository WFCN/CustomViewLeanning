<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PaintStyleTestView">
        <attr name="paint_style" format="enum">
            <enum name="Style_Full" value="1"/>
            <enum name="Style_Full_Stroke" value="2"/>
            <enum name="Style_Stroke" value="3"/>
        </attr>
        <attr name="paint_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="DrawPointView">
        <attr name="dpv_point_color" format="color"/>
        <attr name="dpv_point_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="DrawRectView">
        <attr name="drv_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="DrawRoundRect">
        <attr name="drr_rx" format="dimension"/>
        <attr name="drr_ry" format="dimension"/>
    </declare-styleable>
    <attr name="line_color" format="color"/>
    <attr name="shape_color" format="color"/>
    <attr name="shape_width" format="dimension"/>
    <attr name="shape_height" format="dimension"/>

    <declare-styleable name="DrawOvalView">
        <attr name="line_color"/>
    </declare-styleable>
    <declare-styleable name="DrawArcView">
        <attr name="line_color"/>
        <attr name="shape_color"/>
    </declare-styleable>
</resources>